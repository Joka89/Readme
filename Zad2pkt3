using System;

namespace Zadanie3
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, wynik;

            Console.WriteLine("Program mnoży dwie liczby");
            Console.WriteLine("Podaj liczbę a");
            a = int.Parse(Console.ReadLine());

            Console.WriteLine("Podaj liczbę b");
            b = int.Parse(Console.ReadLine());

            wynik = a * b;

            Console.WriteLine("Wynik mnożenia liczb " + a + " i " + b + " wynosi " + wynik);
        }
    }
}
